        RowBox[{
          RowBox[{"f", "[", 
            RowBox[{"n_", ",", "x_"}], "]"}], ":=", 
          RowBox[{
            RowBox[{"(", 
              RowBox[{"x", "+", 
                RowBox[{
                  RowBox[{"f", "[", 
                    RowBox[{
                    RowBox[{"n", "-", "1"}], ",", "x"}], "]"}], "^", "2"}]}], 
              ")"}], "/", 
            RowBox[{"(", 
              RowBox[{"2", " ", 
                RowBox[{"f", "[", 
                  RowBox[{
                    RowBox[{"n", "-", "1"}], ",", "x"}], "]"}]}], 
              ")"}]}]}]}]], "ChatCode", "Input", LanguageCategory -> "Input", 
