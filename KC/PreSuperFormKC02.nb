   Concatenate @@ (ReduceSetList[IntegerDigits[myInt]] /. {
       IndexedConcatenate[a__ /; Length[{a}] > 1, n_] :>
        RowBox[{OverscriptBox[\"\[Euro]\", n], \"(\", FromDigits[{a}], \")\"}],
       IndexedConcatenate[a__ /; Length[{a}] == 1, n_] :>
        RowBox[{OverscriptBox[\"\[Euro]\", n], FromDigits[{a}]}] } )];\
\>", "Text",
