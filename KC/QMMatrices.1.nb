Cell[CellGroupData[{

Cell["Math with I-Cats:", "Subsubsection",
 CellChangeTimes->{{3.9532916256241474`*^9, 
  3.95329163388735*^9}},ExpressionUUID->"0379e45d-1126-e147-93c6-\
7c89a50ea87e"],

Cell[TextData[{
 "How to add ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["a", "\[Dagger]"], TraditionalForm]],ExpressionUUID->
  "26c6c3de-1992-eb48-a362-4fb5ae652c43"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "a"}], TraditionalForm]],ExpressionUUID->
  "481f3ef5-a2c2-a642-b6c7-6be96f8b83f1"],
 "?  We need to partially expand one or both of the compressed expressions, \
so the pieces have the same index names, starting values and ending values. \
(This is not a new concept, it corresponds to what is commonly done when \
combining sums written in summation notation.) "
}], "Text",
 CellChangeTimes->{{3.948297088894585*^9, 3.948297133070219*^9}, {
  3.9532973234028034`*^9, 
  3.953297434455078*^9}},ExpressionUUID->"ae239aff-148e-cb46-a878-\
d6930e9a9c31"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["a", "\[Dagger]"], "+", "a"}], 
  TraditionalForm]],ExpressionUUID->"b7047c6d-46c2-4145-95f7-5cd853afc247"]], \
"Text",
 CellChangeTimes->{{3.948297088894585*^9, 3.948297152659291*^9}, {
  3.948299902028871*^9, 
  3.948299933590585*^9}},ExpressionUUID->"3d9c244f-3254-7842-9703-\
9f835f191692"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{"{", 
     TagBox[
      RowBox[{
       UnderoverscriptBox["\[Euro]", 
        RowBox[{"k", "\[DoubleRightTee]", "1"}], "\[Infinity]"], "[", 
       RowBox[{"{", 
        RowBox[{
         TagBox[
          RowBox[{
           OverscriptBox["\[Euro]", 
            RowBox[{"k", "-", "2"}]], "[", "0", "]"}],
          DisplayForm], ",", 
         SqrtBox[
          RowBox[{"k", "-", "1"}]], ",", 
         TagBox[
          RowBox[{
           OverscriptBox["\[Euro]", "\[Infinity]"], "[", "0", "]"}],
          DisplayForm]}], "}"}], "]"}],
      DisplayForm], "}"}], "+", 
    TagBox[
     RowBox[{"{", 
      TagBox[
       RowBox[{
        UnderoverscriptBox["\[Euro]", 
         RowBox[{"k", "\[DoubleRightTee]", "1"}], "\[Infinity]"], "[", 
        RowBox[{"{", 
         RowBox[{
          TagBox[
           RowBox[{
            OverscriptBox["\[Euro]", "k"], "[", "0", "]"}],
           DisplayForm], ",", 
          SqrtBox["k"], ",", 
          TagBox[
           RowBox[{
            OverscriptBox["\[Euro]", "\[Infinity]"], "[", "0", "]"}],
           DisplayForm]}], "}"}], "]"}],
       DisplayForm], "}"}],
     DisplayForm]}]}], 
  TraditionalForm]],ExpressionUUID->"a6fbf2b2-5f27-2941-9659-24f9562950c2"]], \
"Text",
 CellChangeTimes->{{3.948297088894585*^9, 3.948297152659291*^9}, {
  3.948299902028871*^9, 
  3.9482999375002537`*^9}},ExpressionUUID->"aa632bc1-c6c6-9e47-9f46-\
89d42dc83cb9"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"=", 
   RowBox[{"{", 
